class GFG 
{ 
	static int calculateSum(int n) 
	{ 
		// Sn = n*(4*n*n + 6*n - 1)/3 
		return (n * (4 * n * n + 
					6 * n - 1) / 3); 
	} 
	public static void main(String args[]) 
	{ 
		// number of terms to be 
		// included in the sum 
		int n = 4; 
	
		// find the Sn 
		System.out.println("Sum = " + 
							calculateSum(n)); 
	} 
} 


